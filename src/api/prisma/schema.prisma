generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = "-1"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int       @id @default(autoincrement())
  email   String    @unique
  name    String?
  Project Project[]
}

model Project {
  id                         String   @id @default(nanoid())
  title                      String
  description                String
  lastModified               DateTime @updatedAt
  isozones_taskid            String   @default("")
  isozones_running           Boolean  @default(false)
  catchment_geojson          String   @default("")
  catchment_area             Float    @default(0)
  branches_geojson           String   @default("")
  channel_length             Float    @default(0)
  cummulative_channel_length Float    @default(0)
  delta_h                    Float    @default(0)
  pointId                    Int
  Point                      Point    @relation(fields: [pointId], references: [id], onDelete: Cascade)
  userId                     Int
  user                       User     @relation(fields: [userId], references: [id])

  idfParameterId Int?
  IDF_Parameters IDF_Parameters?  @relation(fields: [idfParameterId], references: [id], onDelete: Cascade)
  Mod_Fliesszeit Mod_Fliesszeit[]
  Koella Koella[]
  ClarkWSL ClarkWSL[]
  NAM NAM[]
}

model Point {
  id       Int       @id @default(autoincrement())
  northing Float
  easting  Float
  Project  Project[]
}

model IDF_Parameters {
  id         Int       @id @default(autoincrement())
  P_low_1h   Float
  P_high_1h  Float
  P_low_24h  Float
  P_high_24h Float
  rp_low     Float
  rp_high    Float
  Project    Project[]
}

model Mod_Fliesszeit {
  id                          Int                    @id @default(autoincrement())
  x                           Int
  Annuality                   Annualities            @relation(fields: [x], references: [id])
  Vo20                        Float
  psi                         Float
  TB_start                    Int                    @default(30)
  istep                       Int                    @default(5)
  tol                         Int                    @default(5)
  max_iter                    Int                    @default(1000)
  project_id                  String
  Project                     Project                @relation(fields: [project_id], references: [id], onDelete: Cascade)
  mod_fliesszeit_result_id    Int?                   @unique
  Mod_Fliesszeit_Result       Mod_Fliesszeit_Result? @relation(name: "current", fields: [mod_fliesszeit_result_id], references: [id], onDelete: Cascade)
  mod_fliesszeit_result_1_5_id Int?                   @unique
  Mod_Fliesszeit_Result_1_5   Mod_Fliesszeit_Result? @relation(name:"1_5_degree", fields: [mod_fliesszeit_result_1_5_id], references: [id], onDelete: Cascade)
  mod_fliesszeit_result_2_id  Int?                   @unique
  Mod_Fliesszeit_Result_2     Mod_Fliesszeit_Result? @relation(name:"2_degree", fields: [mod_fliesszeit_result_2_id], references: [id], onDelete: Cascade)
  mod_fliesszeit_result_3_id  Int?                   @unique
  Mod_Fliesszeit_Result_3     Mod_Fliesszeit_Result? @relation(name:"3_degree", fields: [mod_fliesszeit_result_3_id], references: [id], onDelete: Cascade)
  mod_fliesszeit_result_4_id  Int?                   @unique
  Mod_Fliesszeit_Result_4     Mod_Fliesszeit_Result? @relation(name:"4_degree", fields: [mod_fliesszeit_result_4_id], references: [id], onDelete: Cascade)
}

model Mod_Fliesszeit_Result {
  id             Int             @id @default(autoincrement())
  HQ             Float
  Tc             Float
  TB             Float
  TFl            Float
  i              Float
  Vox            Float
  Mod_Fliesszeit                    Mod_Fliesszeit? @relation("current")
  Mod_Fliesszeit_1_5                Mod_Fliesszeit? @relation("1_5_degree")
  Mod_Fliesszeit_2                  Mod_Fliesszeit? @relation("2_degree")
  Mod_Fliesszeit_3                  Mod_Fliesszeit? @relation("3_degree")
  Mod_Fliesszeit_4                  Mod_Fliesszeit? @relation("4_degree")
}

model Koella {
  id                       Int                    @id @default(autoincrement())
  x                        Int
  Annuality                Annualities            @relation(fields: [x], references: [id])
  Vo20                     Float
  glacier_area             Int
  TB_start                 Int                    @default(30)
  tol                      Int                    @default(5)
  istep                    Int                    @default(5)
  max_iter                 Int                    @default(1000)
  project_id               String
  Project                  Project                @relation(fields: [project_id], references: [id], onDelete: Cascade)  
  koella_result_id         Int?                   @unique
  Koella_Result            Koella_Result? @relation(name:"current", fields: [koella_result_id], references: [id], onDelete: Cascade)
  koella_result_1_5_id     Int?                   @unique
  Koella_Result_1_5        Koella_Result? @relation(name:"1_5_degree", fields: [koella_result_1_5_id], references: [id], onDelete: Cascade)
  koella_result_2_id       Int?                   @unique
  Koella_Result_2          Koella_Result? @relation(name: "2_degree", fields: [koella_result_2_id], references: [id], onDelete: Cascade)
  koella_result_3_id       Int?                   @unique
  Koella_Result_3          Koella_Result? @relation(name: "3_degree", fields: [koella_result_3_id], references: [id], onDelete: Cascade)
  koella_result_4_id       Int?                   @unique
  Koella_Result_4            Koella_Result? @relation(name: "4_degree", fields: [koella_result_4_id], references: [id], onDelete: Cascade)
}

model Koella_Result {
  id             Int             @id @default(autoincrement())
  HQ             Float
  Tc             Float
  TB             Float
  TFl            Float
  FLeff          Float
  i_final        Float
  i_korrigiert   Float
  Koella                    Koella? @relation("current")
  Koella_1_5                Koella? @relation("1_5_degree")
  Koella_2                  Koella? @relation("2_degree")
  Koella_3                  Koella? @relation("3_degree")
  Koella_4                  Koella? @relation("4_degree")
}


model ClarkWSL {
  id                       Int                    @id @default(autoincrement())
  x                        Int
  Annuality                Annualities            @relation(fields: [x], references: [id])
  dt                       Int                    @default(10)
  pixel_area_m2            Int                    @default(25)
  Fractions                Fractions[]
  project_id               String
  Project                  Project                @relation(fields: [project_id], references: [id], onDelete: Cascade)
  clarkwsl_result_id       Int?                   @unique
  ClarkWSL_Result          ClarkWSL_Result? @relation(name: "current", fields: [clarkwsl_result_id], references: [id], onDelete: Cascade)
  clarkwsl_result_1_5_id   Int?                   @unique
  ClarkWSL_Result_1_5      ClarkWSL_Result? @relation(name: "1_5_degree", fields: [clarkwsl_result_1_5_id], references: [id], onDelete: Cascade)
  clarkwsl_result_2_id    Int?                   @unique
  ClarkWSL_Result_2        ClarkWSL_Result? @relation(name: "2_degree", fields: [clarkwsl_result_2_id], references: [id], onDelete: Cascade)
  clarkwsl_result_3_id     Int?                   @unique
  ClarkWSL_Result_3        ClarkWSL_Result? @relation(name: "3_degree", fields: [clarkwsl_result_3_id], references: [id], onDelete: Cascade)
  clarkwsl_result_4_id     Int?                   @unique
  ClarkWSL_Result_4        ClarkWSL_Result? @relation(name: "4_degree", fields: [clarkwsl_result_4_id], references: [id], onDelete: Cascade)
}

model Fractions {
  id                       Int                    @id @default(autoincrement())
  ZoneParameterTyp         String
  ZoneParameter            ZoneParameter @relation(fields: [ZoneParameterTyp], references: [typ])
  pct                      Float
  clarkwsl_id              Int 
  ClarkWSL                 ClarkWSL @relation(fields: [clarkwsl_id], references: [id], onDelete: Cascade)
}

model ZoneParameter {
  typ                      String   @id
  V0_20                    Float
  WSV                      Float
  psi                      Float
  alpha                    Float
  Fractions                Fractions[]
}

model ClarkWSL_Result {
  id             Int             @id @default(autoincrement())
  Q             Float
  W             Float
  K             Float
  Tc            Float
  ClarkWSL                    ClarkWSL? @relation("current")
  ClarkWSL_1_5                ClarkWSL? @relation("1_5_degree")
  ClarkWSL_2                  ClarkWSL? @relation("2_degree")
  ClarkWSL_3                  ClarkWSL? @relation("3_degree")
  ClarkWSL_4                  ClarkWSL? @relation("4_degree")
}

model NAM {
  id                       Int                    @id @default(autoincrement())
  x                        Int
  Annuality                Annualities            @relation(fields: [x], references: [id])
  precipitation_factor     Float                  @default(0.7)
  water_balance_mode       String                 @default("cumulative")
  WaterBalanceMode         WaterBalanceMode       @relation(fields: [water_balance_mode], references: [mode])
  storm_center_mode        String                 @default("centroid")
  StormCenterMode          StormCenterMode        @relation(fields: [storm_center_mode], references: [mode])
  routing_method           String                 @default("time_values")
  RoutingMethod            RoutingMethod          @relation(fields: [routing_method], references: [method])
  readiness_to_drain       Int                    @default(0)
  use_own_soil_data        Boolean                @default(false)
  project_id               String
  Project                  Project                @relation(fields: [project_id], references: [id], onDelete: Cascade)
  nam_result_id            Int?                   @unique
  NAM_Result               NAM_Result? @relation(name: "current", fields: [nam_result_id], references: [id], onDelete: Cascade)
  nam_result_1_5_id        Int?                   @unique
  NAM_Result_1_5           NAM_Result? @relation(name: "1_5_degree", fields: [nam_result_1_5_id], references: [id], onDelete: Cascade)
  nam_result_2_id          Int?                   @unique
  NAM_Result_2             NAM_Result? @relation(name: "2_degree", fields: [nam_result_2_id], references: [id], onDelete: Cascade)
  nam_result_3_id          Int?                   @unique
  NAM_Result_3             NAM_Result? @relation(name: "3_degree", fields: [nam_result_3_id], references: [id], onDelete: Cascade)
  nam_result_4_id          Int?                   @unique
  NAM_Result_4             NAM_Result? @relation(name: "4_degree", fields: [nam_result_4_id], references: [id], onDelete: Cascade)
}

model NAM_Result {
  id                     Int             @id @default(autoincrement())
  HQ                     Float
  Tc                     Float
  TB                     Float
  TFl                    Float
  i                      Float
  S                      Float
  Ia                     Float
  Pe                     Float
  HQ_time                String?         
  effective_curve_number Float?
  NAM                    NAM? @relation("current")
  NAM_1_5                NAM? @relation("1_5_degree")
  NAM_2                  NAM? @relation("2_degree")
  NAM_3                  NAM? @relation("3_degree")
  NAM_4                  NAM? @relation("4_degree")
}

model Annualities {
  id             Int              @id @default(autoincrement())
  description    String
  number         Float            @unique
  Mod_Fliesszeit Mod_Fliesszeit[]
  Koella Koella[]
  ClarkWSL ClarkWSL[]
  NAM NAM[]
}

model Precipitation {
  id              Int            @id @default(autoincrement())
  latitude        Float
  longitude       Float
  data            String
}

model WaterBalanceMode {
  mode            String   @id
  description     String
  NAM             NAM[]
}

model StormCenterMode {
  mode            String   @id
  description     String
  NAM             NAM[]
}

model RoutingMethod {
  method          String   @id
  description     String
  NAM             NAM[]
}