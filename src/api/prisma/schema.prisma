generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = "-1"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int       @id @default(autoincrement())
  email   String    @unique
  name    String?
  Project Project[]
}

model Project {
  id                         String   @id @default(nanoid())
  title                      String
  description                String
  lastModified               DateTime @updatedAt
  isozones_taskid            String   @default("")
  isozones_running           Boolean  @default(false)
  catchment_geojson          String   @default("")
  catchment_area             Float    @default(0)
  branches_geojson           String   @default("")
  channel_length             Float    @default(0)
  cummulative_channel_length Float    @default(0)
  delta_h                    Float    @default(0)
  pointId                    Int
  Point                      Point    @relation(fields: [pointId], references: [id], onDelete: Cascade)
  userId                     Int
  user                       User     @relation(fields: [userId], references: [id])

  idfParameterId Int?
  IDF_Parameters IDF_Parameters?  @relation(fields: [idfParameterId], references: [id], onDelete: Cascade)
  Mod_Fliesszeit Mod_Fliesszeit[]
  Koella Koella[]
  ClarkWSL ClarkWSL[]
}

model Point {
  id       Int       @id @default(autoincrement())
  northing Float
  easting  Float
  Project  Project[]
}

model IDF_Parameters {
  id         Int       @id @default(autoincrement())
  P_low_1h   Float
  P_high_1h  Float
  P_low_24h  Float
  P_high_24h Float
  rp_low     Float
  rp_high    Float
  Project    Project[]
}

model Mod_Fliesszeit {
  id                       Int                    @id @default(autoincrement())
  x                        Int
  Annuality                Annualities            @relation(fields: [x], references: [id])
  Vo20                     Float
  psi                      Float
  TB_start                 Int                    @default(30)
  istep                    Int                    @default(5)
  tol                      Int                    @default(5)
  max_iter                 Int                    @default(1000)
  project_id               String
  Project                  Project                @relation(fields: [project_id], references: [id])
  mod_fliesszeit_result_id Int?                   @unique
  Mod_Fliesszeit_Result    Mod_Fliesszeit_Result? @relation(fields: [mod_fliesszeit_result_id], references: [id], onDelete: Cascade)
}

model Mod_Fliesszeit_Result {
  id             Int             @id @default(autoincrement())
  HQ             Float
  Tc             Float
  TB             Float
  TFl            Float
  i              Float
  Vox            Float
  Mod_Fliesszeit Mod_Fliesszeit?
}

model Koella {
  id                       Int                    @id @default(autoincrement())
  x                        Int
  Annuality                Annualities            @relation(fields: [x], references: [id])
  Vo20                     Float
  glacier_area             Int
  TB_start                 Int                    @default(30)
  tol                      Int                    @default(5)
  istep                    Int                    @default(5)
  max_iter                 Int                    @default(1000)
  project_id               String
  Project                  Project                @relation(fields: [project_id], references: [id])
  koella_result_id         Int?                   @unique
  Koella_Result            Koella_Result? @relation(fields: [koella_result_id], references: [id], onDelete: Cascade)
}

model Koella_Result {
  id             Int             @id @default(autoincrement())
  HQ             Float
  Tc             Float
  TB             Float
  TFl            Float
  FLeff          Float
  i_final        Float
  i_korrigiert   Float
  Koella  Koella?
}


model ClarkWSL {
  id                       Int                    @id @default(autoincrement())
  x                        Int
  Annuality                Annualities            @relation(fields: [x], references: [id])
  dt                       Int                    @default(10)
  pixel_area_m2            Int                    @default(25)
  Fractions                Fractions[]
  project_id               String
  Project                  Project                @relation(fields: [project_id], references: [id])
  clarkwsl_result_id       Int?                   @unique
  ClarkWSL_Result          ClarkWSL_Result? @relation(fields: [clarkwsl_result_id], references: [id], onDelete: Cascade)
}

model Fractions {
  id                       Int                    @id @default(autoincrement())
  ZoneParameterTyp         String
  ZoneParameter            ZoneParameter @relation(fields: [ZoneParameterTyp], references: [typ])
  pct                      Float
  clarkwsl_id              Int 
  ClarkWSL                 ClarkWSL @relation(fields: [clarkwsl_id], references: [id])
}

model ZoneParameter {
  typ                      String   @id
  V0_20                    Float
  WSV                      Float
  psi                      Float
  alpha                    Float
  Fractions                Fractions[]
}

model ClarkWSL_Result {
  id             Int             @id @default(autoincrement())
  Q             Float
  W             Float
  K             Float
  Tc            Float
  ClarkWSL  ClarkWSL?
}

model Annualities {
  id             Int              @id @default(autoincrement())
  description    String
  number         Float            @unique
  Mod_Fliesszeit Mod_Fliesszeit[]
  Koella Koella[]
  ClarkWSL ClarkWSL[]
}

model Precipitation {
  id              Int            @id @default(autoincrement())
  latitude        Float
  longitude       Float
  data            String
}