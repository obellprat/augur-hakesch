generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = "-1"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int       @id @default(autoincrement())
  email   String    @unique
  name    String?
  Project Project[]
}

model Project {
  id                         String   @id @default(nanoid())
  title                      String
  description                String
  lastModified               DateTime @updatedAt
  isozones_taskid            String   @default("")
  isozones_running           Boolean  @default(false)
  catchment_geojson          String   @default("")
  catchment_area             Float    @default(0)
  branches_geojson           String   @default("")
  channel_length             Float    @default(0)
  cummulative_channel_length Float    @default(0)
  delta_h                    Float    @default(0)
  pointId                    Int
  Point                      Point    @relation(fields: [pointId], references: [id], onDelete: Cascade)
  userId                     Int
  user                       User     @relation(fields: [userId], references: [id])

  idfParameterId Int?
  IDF_Parameters IDF_Parameters?  @relation(fields: [idfParameterId], references: [id], onDelete: Cascade)
  Mod_Fliesszeit Mod_Fliesszeit[]
}

model Point {
  id       Int       @id @default(autoincrement())
  northing Float
  easting  Float
  Project  Project[]
}

model IDF_Parameters {
  id         Int       @id @default(autoincrement())
  P_low_1h   Float
  P_high_1h  Float
  P_low_24h  Float
  P_high_24h Float
  rp_low     Float
  rp_high    Float
  Project    Project[]
}

model Mod_Fliesszeit {
  id                    Int                    @id @default(autoincrement())
  x                     Int
  Annuality             Annualities            @relation(fields: [x], references: [id])
  Vo20                  Float
  psi                   Float
  TB_start              Int                    @default(30)
  istep                 Int                    @default(5)
  tol                   Int                    @default(5)
  max_iter              Int                    @default(1000)
  project_id            String                 @unique
  Project               Project                @relation(fields: [project_id], references: [id])
  Mod_Fliesszeit_Result Mod_Fliesszeit_Result?
}

model Mod_Fliesszeit_Result {
  id             Int            @id @default(autoincrement())
  HQ             Float
  Tc             Float
  TB             Float
  TFl            Float
  i              Float
  Vox            Float
  mod_fliesszeit Int            @unique
  Mod_Fliesszeit Mod_Fliesszeit @relation(fields: [mod_fliesszeit], references: [id])
}

model Annualities {
  id             Int              @id @default(autoincrement())
  description    String
  Mod_Fliesszeit Mod_Fliesszeit[]
  number         Float            @unique
}
